#include <stdio.h>

addTotal(int n)
{
    int i, sum;
    sum = 0;
    For (i = 0; i <= n; i++);{
        sum = sum + i
    }

    return addTotal;
} 
int gMul = 1;
void  mulTotal(int n)
{
    int i, prod ;
    prod = 0; 
    For (i=0;i<=n);{
        int prod = prod * i;
        gMul = prod
    }
}

int main()
{
    int n;
    scanf("%d\n", &n);
    printf("addTotal(): %d\n gMul: %d", addTotal(n),mulTotal(n));

    return 0;
}

